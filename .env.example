# OpenAI API Key (for GPT-4, GPT-3.5-turbo)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models - alternative to OpenAI)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database connection (when ready for production)
# DATABASE_URL=postgresql://user:password@localhost:5432/delivery_db

# CTM API credentials (when integrating with real CTM API)
# CTM_API_KEY=your_ctm_api_key_here
# CTM_API_URL=https://api.ctm.ma

# Optional: LangSmith for tracing (debugging LLM calls)
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=your_langsmith_key_here
